// prints addresses of an array given a size and name 
void printAddress(arr, size, name) {
	cout << "Data Address   Data Value     Information << endl;

	for (int i = 0; i < size; i++) {
		cout << *arr[i] << "   " arr[i] << "   " 
			<< name << "[" << i << "]"
	}
}
